<!-- Hexcannon -->
<Definition xsi:type="MyObjectBuilder_ConveyorSorterDefinition">
        <Id>
          <TypeId>ConveyorSorter</TypeId>
          <SubtypeId>Hexcannon</SubtypeId>
        </Id>
        <DisplayName>[EXO] Artificial Sun Launcher</DisplayName>
        <Description>
    ☢ CAUTION!! ☢
    Fires a singularity that
    travels for [3km].
    At the end, an artificial
    sun is spawned, with a
    [2km pull EWAR field],
    lasting 1 minute.
    
		Anything pulled to the center 
    will experience
    Ultimate Nuclear Fusion™
    2 minute reload.
        </Description>
        <Icon>Textures\GUI\HexCannon.png</Icon>
        <CubeSize>Large</CubeSize>
        <BlockTopology>TriangleMesh</BlockTopology>
        <Size x="3" y="3" z="12" />
        <ModelOffset x="0" y="0" z="0"/>
