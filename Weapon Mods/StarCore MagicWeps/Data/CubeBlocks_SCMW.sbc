    <CubeBlocks>


<!-- Hakkero Beam -->
<Definition xsi:type="MyObjectBuilder_ConveyorSorterDefinition">
        <Id>
          <TypeId>ConveyorSorter</TypeId>
          <SubtypeId>DisabledHakkeroBeam</SubtypeId>
        </Id>
        <DisplayName>Arcane Octagon (Beam)</DisplayName>
        <Description>
          WIP
          Fires a variety of beam attacks. Will eventually burn out.
          Ammo 1: MasterSpark [EN]
          Ammo 2: HealingSpark [HEAL]
          Ammo 3: StardustLaser [EN]
          Ammo 4: StardustSentry [EN]
        </Description>
        <Icon>Textures\GUI\hakkero.png</Icon>
        <CubeSize>Large</CubeSize>
        <BlockTopology>TriangleMesh</BlockTopology>
        <Size x="9" y="9" z="3" />
        <ModelOffset x="0" y="0" z="0"/>
        <Center x="4" y="4" z="0" />
        <Model>Models\Cubes\Large\Hakkero.mwm</Model>
        <Components>
          <Component Subtype="SteelPlate" Count="300" />
          <Component Subtype="Construction" Count="120" />
          <Component Subtype="MetalGrid" Count="80" />
          <Component Subtype="SmallTube" Count="30" />
          <Component Subtype="LargeTube" Count="60" />
          <Component Subtype="Motor" Count="20" />
          <Component Subtype="Superconductor" Count="120" />
          <Component Subtype="PowerCell" Count="240" />
          <Component Subtype="Computer" Count="30" />
          <Component Subtype="Construction" Count="100" />
          <Component Subtype="SteelPlate" Count="3500" />
        </Components>
        <CriticalComponent Subtype="Computer" Index="0" />
			<MountPoints>
				<MountPoint Side="Back" StartX="0.00" StartY="0.00" EndX="9.00" EndY="9.00"/>
				<MountPoint Side="Left" StartX="0.00" StartY="0.00" EndX="2.00" EndY="9.00"/>
				<MountPoint Side="Right" StartX="1.00" StartY="0.00" EndX="3.00" EndY="9.00"/>
				<MountPoint Side="Top" StartX="0.00" StartY="1.00" EndX="9.00" EndY="3.00"/>
				<MountPoint Side="Bottom" StartX="0.00" StartY="0.00" EndX="9.00" EndY="2.00"/>
			</MountPoints>
        <StandbyPowerConsumption>1</StandbyPowerConsumption>
        <BlockPairName>HBEAM</BlockPairName>
        <MirroringY>Z</MirroringY>
        <MirroringZ>Y</MirroringZ>
        <BuildTimeSeconds>600</BuildTimeSeconds>
        <EdgeType>Heavy</EdgeType>
        <WeaponDefinitionId Subtype="HBEAM" />
        <InventoryMaxVolume>0.960</InventoryMaxVolume>
        <ResourceSinkGroup>Defense</ResourceSinkGroup>
        <DamageEffectId>213</DamageEffectId>
        <DeformationRatio>0.2</DeformationRatio>
        <PCU>1800</PCU>
      </Definition>

